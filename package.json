{
  "name": "vscode-bw-block-editor",
  "displayName": "BW Block Editor",
  "description": "The editor for BW blocks",
  "version": "0.1.0",
  "publisher": "Yoshiya Hinosawa",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kt3k/vscode-bw-block-editor"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "kt3k.bwBlock",
        "displayName": "Block",
        "selector": [
          {
            "filenamePattern": "*.block.json"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "deno -A jsr:@kt3k/pack@0.1.14 -o out/extension.js src/extension.ts --external vscode --format cjs"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/vscode": "^1.73.0"
  }
}
